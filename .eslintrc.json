{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
      "jquery": true
    },
    "globals": {
      "document": true,
      "window": true,
      "location": true,
      "google": true
    },
    "rules": {
      "strict": 0,
      "semi": 0,
      "no-console": 0,
      "arrow-parens": 0,
      "comma-dangle": 0,
      "max-len": [2, 150, 4, {"ignoreUrls": true}],
      "import/no-extraneous-dependencies": 0
    }
}
